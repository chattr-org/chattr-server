{
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "impliedStrict": true
        },
        "allowImportExportEverywhere": false
    },
    "env": {
        "node": true
    },
    "extends": "eslint:recommended",
    "rules": {
        "curly": "warn",
        "eqeqeq": "warn",
        "default-case": "warn",
        "strict": "error",
        "camelcase": "warn",
        "semi": "warn",
        "no-console": "off"
    }
}
